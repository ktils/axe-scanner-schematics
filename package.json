{
  "name": "axe-scanner-schematics",
  "version": "0.1.0",
  "description": "A schematic that contains custom component generate, with optional rxjs management",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "add": "npm run build && sudo npm link",
    "open": "open -a 'Visual Studio Code' '$f'"
  },
  "keywords": [
    "schematics"
  ],
  "ng-add": {
    "save": "devDependencies"
  },
  "author": "Lakunta Tillis",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^11.2.1",
    "@angular-devkit/schematics": "^11.2.1",
    "@schematics/angular": "^11.2.1",
    "jasmine-axe": "^1.1.0",
    "rxjs": "^6.6.6",
    "typescript": "~4.1.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/node": "^12.11.1",
    "jasmine": "~3.8.0"
  },

  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktils/axe-scan-generator.git"
  },
  "bugs": {
    "url": "https://github.com/ktils/axe-scan-generator/issues"
  },
  "homepage": "https://github.com/ktils/axe-scan-generator/blob/main/axe-scanner/README.md"
}
